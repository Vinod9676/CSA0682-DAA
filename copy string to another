#include <stdio.h>
int main() {
    char str1[100], str2[100];
    int i;
    printf("Enter string str1: ");
    scanf("%s", str1);
    for (i = 0; str1[i] != '\0'; ++i) {
        str2[i] = str1[i];
    }
    str2[i] = '\0';
    printf("str1 = %s\n", str1);
    printf("str2 = %s\n", str2);
    return 0;
}
